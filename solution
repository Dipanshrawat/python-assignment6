def visiblebottles(a):
  b=1
  if len(a)==len(set(a)):
    return b
  while a:
    d=[x for x in set(a) for i in range((a.count(x)-1)) if a.count(x)>1]
    #p+=[list(set(d))[-1]]
    if len(d) !=len(set(d)):
      b+=a
      a=d
    else:
      return b+1
n=int(input())
c=list(map(int,input().split()))
print(visiblebottles(c))
